[project]
	description = Mirror of Apache James Project
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/cotton (jerry-i)
[access "refs/for/refs/*"]
	read = group user/cotton (jerry-i)
	push = group user/cotton (jerry-i)
	label-Code-Review = -2..+2 group user/cotton (jerry-i)
	label-Verified = -1..+1 group user/cotton (jerry-i)
	rebase = group user/cotton (jerry-i)
	removeReviewer = group user/cotton (jerry-i)
	submit = group user/cotton (jerry-i)
[access "refs/heads/*"]
	read = group user/cotton (jerry-i)
	create = group user/cotton (jerry-i)
	push = +force group user/cotton (jerry-i)
	pushMerge = group user/cotton (jerry-i)
[access "refs/tags/*"]
	push = +force group user/cotton (jerry-i)
	pushTag = group user/cotton (jerry-i)
	pushSignedTag = group user/cotton (jerry-i)
